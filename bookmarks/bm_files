
# config files

cfsa $XDG_CONFIG_HOME/shell/aliases
cfsp $XDG_CONFIG_HOME/shell/profile
cfzr $XDG_CONFIG_HOME/zsh/.zshrc

cfd $XDG_CONFIG_HOME/dunst/dunstrc
cfv $XDG_CONFIG_HOME/nvim/init.vim
cfvf $XDG_CONFIG_HOME/vifm/vifmrc
cfz $XDG_CONFIG_HOME/zathura/zathurarc

cfdm ~/.local/src/dmenu/config.h
cfdw ~/.local/src/dwm/config.h
cfst ~/.local/src/st/config.h
cfsu ~/.local/src/surf/config.h
cft ~/.local/src/tabbed/config.h

cfbf $HOME/.local/scripts/bookmarks/bm_files
cfbd $HOME/.local/scripts/bookmarks/bm_dirs


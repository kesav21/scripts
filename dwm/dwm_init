#!/bin/sh

# set wallpaper
hsetroot -solid "#282828" & # gruvbox

# sets the statusbar
~/.bin/dwm_setstatbar &

# compositor
# do i need this?
compton &

# battery low/full notifications
~/.bin/batt_notifier &

# initialize screensaver
~/.bin/screensaver_init &

# initialize meta scripts
~/.bin/scripts_autocp
~/.bin/scripts_autorm

# notification daemon
dunst &


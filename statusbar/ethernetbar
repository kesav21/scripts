#!/bin/luajit

local fileio = require('kk.fileio')

ifacepath = os.getenv("XDG_CONFIG_HOME") .. '/bin/ethernet_interface'
-- print(ifacepath)
iface = fileio.firstline(ifacepath)
-- print(iface)
statepath = string.format('/sys/class/net/%s/operstate', iface)
-- print(statepath)
state = fileio.firstline(statepath)
-- print(state)

if state == 'up' then print('   ') else print('   ') end
